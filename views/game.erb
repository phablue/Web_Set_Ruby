<div class="container">
  <div class="warning">
    <h4 class="bg-danger"> Sorry, This game only plays on screens wider than 1024px </h4>
    <span class="glyphicon glyphicon-warning-sign" aria-hidden="true"></span>
  </div>
  <h1 class="title"> Web Set Game </h2>
  <div class="board">
    <div class="face-up-cards" data-id="board-cards">
    </div>
  </div>
  <div class="dead-cards" data-id="dis-card">
    <h4> Sets Found </h4>
  </div>
  <div class="notice" data-id="notice">
    <h1 id="title" data-id="title"></h1>
    <h2 id="message" data-id="message"></h2>
    <button type='button' class='btn btn-default btn-lg btn-block' data-id='restart-game'>
      <span class='glyphicon glyphicon-repeat' aria-hidden='true'></span>
      Play Again
    </button>
  </div>
</div>
